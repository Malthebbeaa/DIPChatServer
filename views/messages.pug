html 
    head
        title Chat Forum
        link(rel="stylesheet", href="/css/styles.css") 
    body 
        header 
            include includes/header
        .container
            h2= "Beskeder"
            each t in userchats
                - const subject = chat.find(sub => sub.id == t.chatId)
                p="Emne: " + subject.subject
                p="Besked: " +t.text 